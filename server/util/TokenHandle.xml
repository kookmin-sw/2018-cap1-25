<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace = "cp.orbit.common.util.TokenHandle">

	<select id="tokenKey" parameterType="String" resultType="String">
		SELECT certi_key
		FROM TB_TEACHER
		WHERE memb_id=#{memb_id}
	</select>

</mapper>